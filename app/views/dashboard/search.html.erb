<% title('Search listings') %>

<div class="dashboard-main-content">
  <div class="container-fluid">

    <div class="row">
      <div class="col-12">
        <h1><%= fa_icon('search', text: 'Search listings') %></h1>
        <div id="map"></div>
      </div>
    </div>

  </div>
</div>

<script>
  function initMap() {
    var seoul = {lat: 37.5665, lng: 126.9780};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 6,
      center: seoul
    });
    var marker = new google.maps.Marker({
      position: seoul,
      map: map
    });
  }
</script>


<script async defer src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GOOGLE_MAPS_API_KEY'] %>&callback=initMap" type="text/javascript"></script>
