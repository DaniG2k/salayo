<button type="reset" @click="activeStep--" :disabled="activeStep === 0" class="btn btn-secondary" />
  <i class="fa fa-chevron-left"></i> Previous
</button>
<button type="reset" @click="activeStep++" :disabled="activeStep === stepList.length - 1" class="btn btn-secondary" />
  Next <i class="fa fa-chevron-right"></i>
</button>
